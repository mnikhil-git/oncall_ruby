<!DOCTYPE html>
<html>
	<head>
		<title>Oncall</title>
		<%= render 'layouts/stylesheets' %>
	</head>
	<body>
		<div class="container">
			<%= render 'layouts/header' %>
			<section>
				<% flash.each do |key, value| %>
					<div class="flash <%= key %>"><%= value %></div>
				<% end%>
				<%= yield %>
			</section>
			<%= render 'layouts/footer' %>
			
			<%= debug(params) if Rails.env.development? %>
		</div>
	</body>
</html>
